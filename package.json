{
  "name": "@metaio/meta-cms-benchmark",
  "version": "0.1.1",
  "description": "E2E benchmark scripts for Meta CMS Backend",
  "license": "MIT",
  "keywords": [
    "benchmark",
    "e2e"
  ],
  "scripts": {
    "start": "tsc && npx tsc-alias && node dist/index.js",
    "build": "tsc && npx tsc-alias",
    "build:docs": "typedoc --out docs src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "tsc && npx tsc-alias && jest"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "ololog": "^1.1.175"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.4.4"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Casimir Crystal <casimir.crystal.blue@outlook.com> (https://github.com/casimir-crystal/)"
}
